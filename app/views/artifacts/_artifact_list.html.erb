<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Category</th>
      <th>Description</th>
      <th>Price</th>
      <th colspan="4"></th>
    </tr>
  </thead>

  <tbody>
    <% artifacts.each do |artifact| %>
      <tr>
        <td><%= artifact.name %></td>
        <td><%= artifact.category %></td>
        <td><%= artifact.description %></td>
        <td><%= artifact.price %></td>
        <td><%= link_to 'Show', artifact %></td>
        <%# <td><%= link_to 'Edit', edit_restaurant_path(restaurant)</td> %>
        <%# <td><%= link_to 'Destroy', restaurant, method: :delete, data: { confirm: 'Are you sure?' }</td> %>
      </tr>
    <% end %>
  </tbody>
</table>
